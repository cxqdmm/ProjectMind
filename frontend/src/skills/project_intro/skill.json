{
  "id": "project_intro",
  "name": "项目介绍",
  "version": "0.1.0",
  "description": "在用户咨询某项目的介绍、方案或产品信息时使用",
  "intents": ["product", "intro", "项目", "方案", "产品"],
  "input_schema": {
    "type": "object",
    "properties": {
      "name": { "type": "string", "description": "项目名称" }
    },
    "required": ["name"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "intro": { "type": "string" }
    }
  },
  "permissions": [],
  "examples": [
    {
      "user": "请介绍高端员福",
      "call": { "provider": "project", "tool": "intro", "input": { "name": "高端员福" } }
    }
  ],
  "dependencies": []
}
